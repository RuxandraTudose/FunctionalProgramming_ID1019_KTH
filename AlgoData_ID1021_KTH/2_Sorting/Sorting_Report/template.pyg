public static void better (int [] array, int [] index) {
    int j=-1, i=-1, count = 0;
    while (i < array.length -1 && j < index.length -1) {

        if(index[j+1] < array[i+1])
            j++;
        else {
            if (index[j+1] == array[i+1])
                count++;
            i++;
        }
}}
